{
  "manifest_version": 3,
  "name": "Market Ease Plus",
  "version": "1.0",
  "description": "Hey Am Market Ease Plus",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["insights.html", "insights.bundle.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'",
    "sandbox": "sandbox allow-scripts allow-modals",
    "web_accessible_resources": ["insights.html"],
    "script-src": ["'self'", "https://feedback.fish"]
  }
}
